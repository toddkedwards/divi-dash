# Environment Variables for Divi Dash
# Copy this file to .env.local and fill in your actual values

# Application URLs
NEXTAUTH_URL=http://localhost:3010
NEXT_PUBLIC_APP_URL=http://localhost:3010

# Firebase Configuration
NEXT_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=123456789
NEXT_PUBLIC_FIREBASE_APP_ID=1:123456789:web:abcdef

# Firebase Admin (Server-side)
FIREBASE_ADMIN_PRIVATE_KEY=your_firebase_admin_private_key
FIREBASE_ADMIN_CLIENT_EMAIL=your_firebase_admin_client_email
FIREBASE_ADMIN_PROJECT_ID=your_project_id

# Financial Data APIs
FINNHUB_API_KEY=your_finnhub_api_key
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key
DIVIDEND_COM_API_KEY=your_dividend_com_api_key

# Charles Schwab API (Production)
SCHWAB_CLIENT_ID=your_schwab_client_id
SCHWAB_CLIENT_SECRET=your_schwab_client_secret
SCHWAB_REDIRECT_URI=http://localhost:3010/api/auth/brokerage/callback?brokerage=schwab

# TD Ameritrade API (Production)
TD_AMERITRADE_CLIENT_ID=your_td_client_id@AMER.OAUTHAP
TD_AMERITRADE_REDIRECT_URI=http://localhost:3010/api/auth/brokerage/callback?brokerage=tdameritrade

# Alpaca API (Paper Trading / Production)
ALPACA_API_KEY=your_alpaca_api_key
ALPACA_SECRET_KEY=your_alpaca_secret_key
ALPACA_SANDBOX=true

# Interactive Brokers API
INTERACTIVE_BROKERS_CLIENT_ID=your_ib_client_id
INTERACTIVE_BROKERS_CLIENT_SECRET=your_ib_client_secret

# Robinhood API (Future Implementation)
ROBINHOOD_CLIENT_ID=your_robinhood_client_id
ROBINHOOD_CLIENT_SECRET=your_robinhood_client_secret

# Fidelity API (Future Implementation)
FIDELITY_CLIENT_ID=your_fidelity_client_id
FIDELITY_CLIENT_SECRET=your_fidelity_client_secret

# E*TRADE API (Future Implementation)
ETRADE_CLIENT_ID=your_etrade_client_id
ETRADE_CLIENT_SECRET=your_etrade_client_secret

# Webull API (Future Implementation)
WEBULL_CLIENT_ID=your_webull_client_id
WEBULL_CLIENT_SECRET=your_webull_client_secret

# Payment Processing (Stripe)
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# Encryption and Security
ENCRYPTION_KEY=your_32_character_encryption_key
JWT_SECRET=your_jwt_secret_key

# Database (if using external database)
DATABASE_URL=postgresql://user:password@localhost:5432/divi_dash

# Email Service (for notifications)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_app_password

# News and Data Services
NEWS_API_KEY=your_news_api_key
POLYGON_API_KEY=your_polygon_api_key

# Analytics and Monitoring
GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
SENTRY_DSN=your_sentry_dsn

# Feature Flags
ENABLE_REAL_API=true
ENABLE_PAPER_TRADING=true
ENABLE_NOTIFICATIONS=true
ENABLE_PREMIUM_FEATURES=true

# Development Settings
NODE_ENV=development
LOG_LEVEL=debug 